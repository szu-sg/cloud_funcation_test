{
  "BasicInfo": {
    "Name": "C++",
    "Slug": "native_cpp",
    "Description": "Quickly get started with C++ using the G++ 10 environment and Makefile build tools.",
    "Categories": [
      "Languages"
    ],
    "ProjectType": "native",
    "Languages": [
      "C++"
    ]
  },
  "Runtime": {
    "RuntimeTemplate": "cpp",
    "Envs": {
      "CLOUDIDE_CONFIG_LANGUAGE": "cpp",
      "CLOUDIDE_CONFIG_RUNTIME": "cpp"
    },
    "Commands": {
      "BeforeStart": {
        "Name": ""
      },
      "AfterStart": {
        "Name": ""
      },
      "OnInit": {
        "Name": "cmake build",
        "Script": [
          "/usr/bin/cmake",
          "--no-warn-unused-cli",
          "-DCMAKE_BUILD_TYPE:STRING=Debug",
          "-DCMAKE_EXPORT_COMPILE_COMMANDS:BOOL=TRUE",
          "-DCMAKE_C_COMPILER:FILEPATH=/usr/bin/gcc",
          "-DCMAKE_CXX_COMPILER:FILEPATH=/usr/bin/g++",
          "-S./",
          "-B./build"
        ]
      }
    }
  },
  "Ide": {},
  "Service": {}
}